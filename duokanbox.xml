<?xml   version="1.0"   encoding="utf-8"?>
<!--suppress XmlUnboundNsPrefix, XmlUnboundNsPrefix -->
<duokanbox>

    <!--获取全部分类-->
    <site>
        <id>0</id>
        <name>category</name>
        <main_url>/category</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>0</sub_count>
        <sub>/%s</sub>
    </site>

    <!--获取某分类下的频道-->
    <site>
        <id>1</id>
        <name>category_channels</name>
        <main_url>/category</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>1</sub_count>
        <sub>/%s</sub>
    </site>

    <!--获取频道详情和歌曲列表-->
    <site>
        <id>2</id>
        <name>channel_info</name>
        <main_url>/channel</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>1</sub_count>
        <sub>/%s</sub>
    </site>

    <!--根据id获取歌曲信息-->
    <site>
        <id>3</id>
        <name>song_info</name>
        <main_url>/music</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>10</sub_count>
        <sub>/%s</sub>
    </site>

    <!--根据在线id获取歌曲id-->
    <site>
        <id>4</id>
        <name>get_online_id_by_cp_id</name>
        <main_url>/sid</main_url>
        <param_or_not>0</param_or_not>
        <sub_count>10</sub_count>
        <sub>/%s</sub>
    </site>

    <!--根据歌曲信息搜索歌曲-->
    <site>
        <id>5</id>
        <name>search_songs</name>
        <main_url>/sinfo</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>1</sub_count>
        <sub>/%s</sub>
    </site>

</duokanbox>