<?xml   version="1.0"   encoding="utf-8"?>
<!--suppress XmlUnboundNsPrefix, XmlUnboundNsPrefix -->
<duokanbox>

    <!--获取全部分类-->
    <site>
        <name>category</name>
        <main_url>http://fm.duokanbox.com/category</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>0</sub_count>
        <sub>/%s</sub>
    </site>

    <!--获取某分类下的频道-->
    <site>
        <name>category_channels</name>
        <main_url>http://fm.duokanbox.com/category</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>1</sub_count>
        <sub>/%s</sub>
    </site>

    <!--获取频道详情和歌曲列表-->
    <site>
        <name>channel_info</name>
        <main_url>http://fm.duokanbox.com/channel</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>1</sub_count>
        <sub>/%s</sub>
    </site>

    <!--根据id获取歌曲信息-->
    <site>
        <name>channel_info</name>
        <main_url>http://fm.duokanbox.com/music</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>10</sub_count>
        <sub>/%s</sub>
    </site>

    <!--根据在线id获取歌曲id-->
    <site>
        <name>get_online_id_by_cp_id</name>
        <main_url>http://fm.duokanbox.com/sid</main_url>
        <param_or_not>0</param_or_not>
        <sub_count>10</sub_count>
        <sub>/%s</sub>
    </site>

    <!--根据歌曲信息搜索歌曲-->
    <site>
        <name>search_songs</name>
        <main_url>http://fm.duokanbox.com/sinfo</main_url>
        <param_or_not>1</param_or_not>
        <sub_count>1</sub_count>
        <sub>/%s</sub>
    </site>

</duokanbox>